  "type": "module",
  "exports": "./index.js",
  "scripts": {
    "start": "npm run clean && npm run prod",
    "docker-start": "cross-env NODE_ENV=production node ./build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-assets": "mkdirp ./dist/assets && cpx ./assets/**/* ./dist/assets",
    "dev-server": "babel-watch ./server/index.js",
    "dev-client": "parcel watch ./client/index.html",
    "dev": "npm run dev-assets && concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "build-assets": "mkdirp ./build/assets && cpx ./server/assets/**/* ./build/assets",
    "build-server": "mkdirp ./build && cross-env NODE_ENV=production babel ./server --out-dir ./build",
    "build-client": "mkdirp ./build/frontend && cross-env NODE_ENV=production parcel build ./client/index.html --out-dir ./build/frontend",
    "build": "npm run build-server && npm run build-client && npm run build-assets",
    "prod": "npm run build && cross-env NODE_ENV=production node ./build/index.js",
    "clean": "shx rm -rf ./.cache ./build ./dist && mkdirp ./.cache ./build ./dist"
  },
