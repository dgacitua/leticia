{
  "name": "leticia-pilot",
  "version": "1.0.0",
  "description": "LeTiCiA Pilot Test",
  "author": "Daniel Gacit√∫a",
  "license": "AGPL-3.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run build && npm run prod",
    "stop": "forever stop ./build/server/index.js && forever stop -c http-server ./build/client -p 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "assets": "mkdirp ./build/assets && cpx ./assets/**/* ./build/assets",
    "build-server": "mkdirp ./build/server && babel ./server --out-dir ./build/server",
    "build-client": "mkdirp ./build/server && parcel build ./client/index.html --out-dir ./build/client",
    "build": "npm run build-server && npm run build-client && npm run assets",
    "prod-server": "forever start ./build/server/index.js",
    "prod-client": "forever start -c http-server ./build/client -p 3000",
    "prod": "npm run prod-server && npm run prod-client",
    "dev-server": "babel-watch ./server/index.js",
    "dev-client": "parcel ./client/index.html --port 3000",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "clean": "rimraf ./.cache && rimraf ./build && rimraf ./dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@websanova/vue-auth": "^3.0.5-beta",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.11.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "human-readable-random-string": "0.0.2",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "mongoose": "^5.9.7",
    "random-username-generator": "^1.0.4",
    "sockjs": "^0.3.20",
    "sockjs-client": "^1.4.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-hot-reload-api": "^2.3.4",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@vue/component-compiler-utils": "^3.1.2",
    "babel-watch": "^7.0.0",
    "concurrently": "^5.1.0",
    "forever": "^2.0.0",
    "http-server": "^0.12.1",
    "mkdirp": "^1.0.4",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
