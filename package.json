{
  "name": "leticia-pilot",
  "version": "1.0.0",
  "description": "LeTiCiA Pilot Test",
  "author": "Daniel Gacit√∫a",
  "license": "AGPL-3.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run build && npm run prod",
    "stop": "forever stop ./build/server/index.js && forever stop -c http-server ./build/client -p 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "assets-prod": "mkdirp ./build/client/assets && cpx ./assets/**/* ./build/client/assets",
    "assets-dev": "mkdirp ./dist/assets && cpx ./assets/**/* ./dist/assets",
    "build-server": "mkdirp ./build/server && cross-env NODE_ENV=production babel ./server --out-dir ./build/server",
    "build-client": "mkdirp ./build/client && cross-env NODE_ENV=production parcel build ./client/index.html --out-dir ./build/client",
    "build": "npm run build-server && npm run build-client && npm run assets-prod",
    "prod-server": "cross-env NODE_ENV=production forever start ./build/server/index.js",
    "prod-client": "cross-env NODE_ENV=production forever start -c http-server ./build/client -p 3000",
    "prod": "npm run prod-server && npm run prod-client",
    "dev-server": "babel-watch ./server/index.js",
    "dev-client": "parcel ./client/index.html --port 3000",
    "dev": "npm run assets-dev && concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "clean": "rimraf ./.cache && rimraf ./build && rimraf ./dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
